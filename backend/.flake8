[flake8]
max-line-length = 100
extend-ignore = E203,W503,F401,D100,D101,D102,D103,D104,D105,D107,D200,D201,D202,D204,D205,D300,D301,D302,D400,D401,D402,D403,D404,D405,D406,D407,D408,D409,D410,D411,D412,D413,D414
per-file-ignores =
    __init__.py:D104
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    .tox,
    .mypy_cache
